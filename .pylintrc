[MAIN]
load-plugins=pylint.extensions.docparams,pylint_pydantic

[MESSAGE CONTROL]
disable= too-many-arguments,
    too-many-locals,
    too-few-public-methods,
    import-outside-toplevel,
    protected-access,

[TYPECHECK]
enable=missing-return-type-doc,
    missing-param-doc,

[BASIC]
no-docstring-rgx=^_
